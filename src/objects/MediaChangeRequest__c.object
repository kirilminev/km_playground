    <validationRules>
        <fullName>Does_Not_Expire_with_no_Expiration_Date</fullName>
        <active>true</active>
        <description>The user left &quot;Never Expires&quot; unchecked but did not enter an expiration date.</description>
        <errorConditionFormula>AND(ISNULL(ExpirationDate__c), NOT(NeverExpires__c))</errorConditionFormula>
        <errorDisplayField>ExpirationDate__c</errorDisplayField>
        <errorMessage>Expiration date cannot be empty if &quot;Never Expires&quot; is unchecked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>High_Priority_Reason</fullName>
        <active>true</active>
        <description>Requires the user to fill out a reason in the High Priority Reason field if high priority is checked.</description>
        <errorConditionFormula>AND(HighPriority__c, ISBLANK(HighPriorityReason__c))</errorConditionFormula>
        <errorDisplayField>HighPriorityReason__c</errorDisplayField>
        <errorMessage>You must enter a reason in the High Priority Reason field in order to mark this request as high priority.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Missing_File_Link_for_Certain_Types</fullName>
        <active>true</active>
        <description>Throws an error if the user selects specific types but does not include a file link.</description>
        <errorConditionFormula>AND(
OR(ISPICKVAL(Type__c, &quot;Clinic Video&quot;), ISPICKVAL(Type__c, &quot;Clinic Sidebar&quot;), ISPICKVAL(Type__c, &quot;Clinic Slides&quot;)),
ISBLANK(FileLink__c))</errorConditionFormula>
        <errorDisplayField>FileLink__c</errorDisplayField>
        <errorMessage>You must include a file link for Clinic Sidebars, Slides, or Videos.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Never_Expires_with_an_Expiration_Date</fullName>
        <active>true</active>
        <description>The user checked &quot;Never Expires&quot; but there is an expiration date filled out.</description>
        <errorConditionFormula>AND(NOT(ISNULL(ExpirationDate__c)), NeverExpires__c)</errorConditionFormula>
        <errorDisplayField>ExpirationDate__c</errorDisplayField>
        <errorMessage>Expiration date cannot be filled out if &quot;Never Expires&quot; is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Patient_Portal_Username_and_Password</fullName>
        <active>true</active>
        <description>Requires the user to enter a Portal Username and Portal Password for the patient portal types.</description>
        <errorConditionFormula>AND(
    ISPICKVAL(Type__c, &quot;Tablet Patient Portals &amp; Websites&quot;),
    OR(ISBLANK(PortalUsername__c), ISBLANK(PortalPassword__c))
)</errorConditionFormula>
        <errorDisplayField>PortalUsername__c</errorDisplayField>
        <errorMessage>You must enter both a portal username and password for this change type.</errorMessage>
    </validationRules>
